programa {
  funcao inicio() {

   inteiro numero
    
   escreva("digite um número: ")
   leia(numero)

   escolha(numero){
    caso 1:{
      escreva("fogo")
      pare
    }
    caso 2:{
      escreva("água")
      pare
    }
    caso 3:{
      escreva("terra")
      pare
    }
    caso 4:{
      escreva("ar")
      pare
    }
    caso contrario:{
      escreva("armaldilha")
      pare
    }
   }

  }
}
